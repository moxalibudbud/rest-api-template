{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@config": "./src/config",
          "@controller": "./src/controller",
          "@lib": "./src/lib",
          "@routes": "./src/routes",
          "@service": "./src/service",
          "@tests": "./src/tests",
          "@utils": "./src/utils"
        }
      }
    ]
  ]
}
